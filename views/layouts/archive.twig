{% extends "base.twig" %}

{% block content %}
    <div class="container">
        <article class="post-type-{{ post.post_type }}" id="post-{{ post.ID }}">
            <section class="article-content">
                <h1 class="article-h1">{{ post.title }}</h1>
                <h2 class="article-h2">{{ post.subtitle }}</h2>
                {{ post.content }}
            </section>
        </article>
    </div> <!-- /.container -->
{% endblock %}